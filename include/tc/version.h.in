#ifndef TC_VERSION_H
#define TC_VERSION_H

/*
 * name of the project (e.g. "libtc")
 */
#define TC_VERSION_NAME "@PROJECT_NAME@"

/*
 * version string of the project (e.g. "2022, 2023")
 */
#define TC_VERSION_STRING "@PROJECT_VERSION@"

/*
 * numeric version of the project (e.g. 2022, 2023)
 */
#define TC_VERSION_NUMBER @PROJECT_VERSION@

/*
 * build number string of the project (e.g. "32")
 */
#define TC_BUILD_STRING "@PROJECT_BUILD@"

/*
 * numeric build number of the project (e.g. 32)
 */
#define TC_BUILD_NUMBER @PROJECT_BUILD@

/*
 * name of the author (e.g. "Thomas Cort")
 */
#define TC_VERSION_AUTHOR "@PROJECT_AUTHOR@"

/*
 * copyright line (e.g. "Copyright (C) 2022, 2023  Thomas Cort")
 */
#define TC_VERSION_COPYRIGHT "@PROJECT_COPYRIGHT@"

/*
 * license lines (e.g. "GPLv3+...")
 */
#define TC_VERSION_LICENSE "@PROJECT_LICENSE@"


#endif
